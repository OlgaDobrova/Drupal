<?php

/**
 * @file
 * Module Contact Mail Hooks.
 */

use Drupal\contact_mail\Hook\Theme;
use Drupal\contact_mail\Hook\MailAlter;

/**
 * Implements hook_mail_alter().
 */
function contact_mail_mail_alter(&$message) {
  MailAlter::hook($message);
}

/**
 * Implements hook_theme().
 */
function contact_mail_theme($existing, $type, $theme, $path) {
  return Theme::hook();
}
